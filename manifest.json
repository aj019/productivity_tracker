{
    "manifest_version": 2,
    "name": "Productivity Tracker Chrom extension",
    "version": "1.0",
    "description": "A simple Chrome extension that tracks productivity",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab","storage","tabs"],
    "background": {
        "scripts": ["background.js"]
    }
}